<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.viewInventoryController">
    <TableView fx:id="inventoryTableView" layoutX="49.0" layoutY="30.0" prefHeight="300.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="itemIDColumn" text="Item ID" />
            <TableColumn fx:id="itemNameColumn" text="Item Name" />
            <TableColumn fx:id="quantityColumn" text="Quantity" />
            <TableColumn fx:id="priceColumn" text="Price" />
            <TableColumn fx:id="itemTypeColumn" text="Item Type" />
            <TableColumn fx:id="SupplierIDColumn" text="Supplier ID" /> <!-- Corrected fx:id -->
        </columns>
    </TableView>
   <Button layoutX="266.0" layoutY="347.0" mnemonicParsing="false" onAction="#gotomanageinventory" style="-fx-background-color: #2F4F4F; -fx-text-fill: white;" text="Go Back" />
</AnchorPane>
